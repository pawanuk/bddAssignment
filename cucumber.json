{
    "default": {
      "paths": ["src/test/features"],
      "require": [
        "src/test/steps/*.ts"
      ],
      "requireModule": [
        "ts-node/register"
      ],
      "formatOptions": {
        "snippetInterface": "async-await"
      },
      "retry": 0, 
      "parallel": 0, 
      "format": [
        "json:reports/cucumber-report.json",
        "html:reports/cucumber-report.html" 
      ]
    }
  }
  